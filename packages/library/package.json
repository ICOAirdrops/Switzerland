{
  "name": "switzerland",
  "description": "Switzerland takes a functional approach to web components using Preact with shadow DOM for style encapsulation, custom elements for interoperability and server-side rendering for universality.",
  "version": "6.1.5",
  "type": "module",
  "types": "./dist/index.client.d.ts",
  "exports": {
    "node": {
      "types": "./dist/index.server.d.ts",
      "default": "./dist/index.server.js"
    },
    "default": {
      "types": "./dist/index.client.d.ts",
      "default": "./dist/index.client.js"
    }
  },
  "dependencies": {
    "preact": "10.13.0",
    "preact-render-to-string": "5.2.6"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.0",
    "@rollup/plugin-typescript": "11.0.0",
    "fslint": "1.1.0",
    "jest-environment-jsdom": "29.4.3",
    "rollup": "3.19.1",
    "ts-loader": "9.4.2"
  },
  "scripts": {
    "build": "make build",
    "format": "prettier . --write",
    "unit": "jest",
    "lint": "make lint",
    "typecheck": "tsc --noEmit",
    "deploy": "make deploy"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [],
        "dependsOn": [
          "^build"
        ]
      },
      "typecheck": {
        "inputs": [
          "default"
        ],
        "outputs": [],
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
